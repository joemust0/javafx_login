    <children>
        <TextField fx:id="usernameField" layoutX="126.0" layoutY="40.0" />
        <PasswordField fx:id="passwordField" layoutX="126.0" layoutY="85.0" />
        <Button fx:id="loginButton" layoutX="126.0" layoutY="130.0" onAction="#handleLoginButtonAction" text="Login" />
    </children>
</AnchorPane>

